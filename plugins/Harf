local function run(msg)
if msg.text == "hi" then
	return "Hello "
end
if msg.text == "Hi" then
	return "Hello "
end
if msg.text == "Hello" then
	return "Hi "
end
if msg.text == "hello" then
	return "Hi "
end
if msg.text == "Salam" then
	return "Salam aleykom"
end
if msg.text == "salam" then
	return "va aleykol asalam"
end
if msg.text == "kir" then
	return "to konet"
end
if msg.text == "mehran" then
	return "kos nanash :D"
end
if msg.text == "fuck" then
	return "you ! Bi adab!"
end
if msg.text == "Max" then
	return "jan?"
end
if msg.text == "bot" then
	return "Hom!?"
end
if msg.text == "Bot" then
	return "Bale?!"
end
if msg.text == "?" then
	return "Hum??"
end
if msg.text == "Bye" then
	return "Good Luck !"
end
if msg.text == "bye" then
	return "Bbye !"
end
end

return {
	description = "Chat With Robot Server", 
	usage = "chat with robot",
	patterns = {
		"^[Hh]i$",
		"^[Hh]ello$",
		"^[Bb]ot$",
		"^[Bb]ye$",
		"^?$",
		"^[tT]ele[sS]ick$",
		"^[Ss]alam$"
		"^[Mm]ax$,
		}, 
	run = run,
    --privileged = true,
	pre_process = pre_process
}
